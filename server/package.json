{
  "name": "openrap-sunbirded-plugin",
  "version": "0.0.20",
  "description": "This plugin provides api's for offline consumption",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node_modules/.bin/rimraf ./dist && tsc --noImplicitUseStrict  && node_modules/.bin/cpy package.json ./dist &&  node_modules/.bin/cpy  db/* dist/db/pouchdb",
    "test": "echo 'go to '../test' folder to execute test cases!'",
    "prepare-data": "node_modules/.bin/rimraf ./dist && tsc --noImplicitUseStrict &&  node ./dist/build/index.js",
    "after-install": "cd node_modules/OpenRAP && npm run build"
  },
  "dependencies": {
    "busboy": "^0.3.0",
    "decompress-zip": "^0.2.2",
    "ejs": "^2.6.1",
    "express-http-proxy": "^1.5.1",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "is-online": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "nano": "^8.0.1",
    "typescript-ioc": "^1.2.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "OpenRAP": "git://github.com/project-sunbird/OpenRAP.git#release-2.1",
    "@project-sunbird/ext-framework-server": "0.1.24",
    "@project-sunbird/telemetry-sdk": "0.0.16",
    "@types/busboy": "^0.2.3",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/hashids": "^1.0.30",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.11.3",
    "@types/unzipper": "^0.9.1",
    "@types/uuid": "^3.4.4",
    "gulp": "^3.9.1",
    "axios": "^0.18.0",
    "cpy-cli": "^2.0.0",
    "rimraf": "^2.6.3",
    "typescript": "^2.7.1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
